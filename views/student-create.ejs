<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <%- include('../include/headscript'); -%>
</head>

<body>
    <!-- begin app -->
    <div class="app">
        <!-- begin app-wrap -->
        <div class="app-wrap">
            <!-- begin pre-loader -->
            <div class="loader">
                <div class="h-100 d-flex justify-content-center">
                    <div class="align-self-center">
                        <img src="assets/img/loader/loader.svg" alt="loader">
                    </div>
                </div>
            </div>
            <!-- end pre-loader -->
            <%- include('../include/header'); -%>
                <!-- begin app-container -->
                <div class="app-container">
                    <%- include('../include/aside'); -%>
                        <!-- end app-navbar -->
                        <!-- begin app-main -->
                        <div class="app-main" id="main">
                            <!-- begin container-fluid -->
                            <div class="container-fluid">
                                <!-- begin row -->
                                <div class="row">
                                    <div class="col-md-12 m-b-30">
                                        <!-- begin page title -->
                                        <div class="d-block d-sm-flex flex-nowrap align-items-center">
                                            <div class="page-title mb-2 mb-sm-0">
                                                <h1>
                                                    <%=title %>
                                                </h1>
                                                <br>
                                            </div>
                                        </div>
                                        <!-- end page title -->
                                    </div>
                                </div>
                                <!-- end row -->
                                <!-- begin row -->
                                <div class="row">
                                    <div class="col-xl-12">
                                        <div class="card card-statistics">
                                            <div class="card-body">
                                                <form method="POST" enctype="multipart/form-data">

                                                    <div class="form-group">
                                                        <label for="studentname">Student Name</label>
                                                        <input type="text" required minlength="3" class="form-control"
                                                            id="studentname" aria-describedby="emailHelp"
                                                            placeholder="Enter Student Name" name="studentname"
                                                            required>
                                                    </div>
                                                    <div class="form-group">
                                                        <label for="adharNo">Adhar Number</label>
                                                        <input type="text" required minlength="12" maxlength="12"
                                                            class="form-control" id="adharNo"
                                                            aria-describedby="emailHelp"
                                                            placeholder="Enter Adhar Number" name="adharNo" required>

                                                    </div>
                                                    <div class="form-group">

                                                        <label for="phoneNo">Phone Number</label>
                                                        <input type="text" required minlength="12" maxlength="12"
                                                            class="form-control" id="phoneNo"
                                                            aria-describedby="emailHelp"
                                                            placeholder="Enter Phone Number" name="phoneNo" required>

                                                    </div>

                                                    <button type="submit" class="btn btn-primary">Submit</button>
                                                </form>
                                            </div>
                                        </div>
                                    </div>

                                </div>
                                <!-- end row -->

                            </div>
                            <!-- end container-fluid -->
                        </div>
                        <!-- end app-main -->
                </div>
                <!-- end app-container -->
                <!-- begin footer -->
                <footer class="footer">
                    <div class="row">
                        <div class="col-12 col-sm-6 text-center text-sm-left">
                            <p>&copy; Copyright 2019. All rights reserved.</p>
                        </div>
                        <div class="col  col-sm-6 ml-sm-auto text-center text-sm-right">
                        </div>
                    </div>
                </footer>
                <!-- end footer -->
        </div>
        <!-- end app-wrap -->
    </div>
    <!-- end app -->
    <%- include('../include/footerscript'); -%>

</body>

</html>